    "\n",
    "def genbernoulli(p):\n",
    "    \n",
    "   \n",
    "    u = random.uniform(0, 1)\n",
    "    if u <= p:\n",
    "        return 1\n",
    "    else:\n",
    "        return 0\n",
    "    \n",
    "p=float(input('enter p'))\n",
    "x=genbernoulli(p)\n",
    "print(x)\n"
